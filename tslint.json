{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "max-classes-per-file": false,
        "interface-name": false,
        "max-line-length": false,
        "trailing-comma": false,
        "quotemark": false,
        "ordered-imports": false,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "member-access": false,
        "arrow-parens": false,
        "radix": false
    }
}
